{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2017",
    "lib": [
      "es2015",
      "dom",
      "dom.iterable",
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "importHelpers": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "baseUrl": "."
  },
  "include": [
  ],
  "exclude": [
    "node_modules"
  ]
}
